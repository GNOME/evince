#, c-format
msgid ""
"%s%s\n"
"Encoding: %s\n"
"%s\n"
"Substituting with <b>%s</b>\n"
"(%s)"
msgstr ""
"%s%s\n"
"รหัสอักขระ: %s\n"
"%s\n"
"แทนที่ด้วย <b>%s</b>\n"
"(%s)"

